<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        Rosarito Ruta del Arte 2018.
    </description>
    <author email="rosaritoartfest@gmail.com" href="www.rosaritoartfest.com">
        Rosarito Art Fest Software Developer Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-minSdkVersion" value="19" />
    <plugin name="cordova-plugin-googlemaps">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyBdRVHowh-nEDviOfqPlcyH4Ywqwoorv0w" />
        <variable name="API_KEY_FOR_IOS" value="AIzaSyBdRVHowh-nEDviOfqPlcyH4Ywqwoorv0w" />
    </plugin>
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="~5.0.4" />
</widget>
